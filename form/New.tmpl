{{ define "New" }}
    {{ template "Header" }}
        {{ template "Menu" }}
        <h2>New Name and Company</h2>
        <form method="POST" action="insert">
            <label>Name</label>
            <input type="text" name="name" />
            
            <br />
            
            <label>Company</label>
            <input type="text" name="company" />

            <br />

            <button type="submit">Save</button>
        </form>
    {{ template "Footer" }}
{{ end }}